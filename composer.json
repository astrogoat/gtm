{
    "name": "astrogoat/gtm",
    "description": "A Google Tag Manager app for Strata",
    "keywords": [
        "astrogoat",
        "laravel",
        "gtm"
    ],
    "homepage": "https://github.com/astrogoat/gtm",
    "license": "MIT",
    "authors": [
        {
            "name": "Brett M",
            "email": "brett@helixsleep.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "astrogoat/strata": "^0.1.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37 || ^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Astrogoat\\Gtm\\": "src",
            "Astrogoat\\Gtm\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Astrogoat\\Gtm\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Astrogoat\\Gtm\\GtmServiceProvider"
            ],
            "aliases": {
                "GoogleTagManager": "Astrogoat\\Gtm\\Facades"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
